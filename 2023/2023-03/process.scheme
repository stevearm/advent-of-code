(define (read-file-to-list filename)
        (call-with-input-file
            filename
            (lambda (p)
                (let f ((x (read-line p)))
                       (if (eof-object? x)
                       '()
                       (cons x (f (read-line p))))))))

; Run the sample. Should print 4361
